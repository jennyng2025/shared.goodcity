{{#init-foundation}}
  {{outlet "notifications"}}
  {{outlet "notification_link"}}
  {{outlet}}
  {{outlet "modal"}}
{{/init-foundation}}

{{#with subscriptions.status as |status|}}
  <div id="status" class="{{if status.online 'online'}} {{if status.hidden 'hidden'}}">
    <i class="fa fa-dot-circle-o"></i>
    <span>{{status.text}}</span>
  </div>
{{/with}}
